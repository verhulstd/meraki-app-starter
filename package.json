{
  "name": "monaca-template-onsenui-v2-js-tabbar",
  "dependencies": {
    "cordova-custom-config": "5.0.2",
    "cordova-plugin-splashscreen": "5.0.1",
    "cordova-plugin-whitelist": "1.3.3",
    "monaca-plugin-monaca-core": "3.2.0",
    "onsenui": "^2.9.2"
  },
  "scripts": {
    "bundlejs":
      "parcel WWW/js/main.js --out-dir WWW/js/ --out-file bundle --public-url ./",
    "bundlecss": "parcel WWW/css/style.scss --out-dir WWW/css --public-url ./",
    "monaca":
      "start monaca preview && start npm run bundlejs && start npm run bundlecss"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "node-sass": "^4.7.2",
    "postcss-modules": "^1.1.0"
  },
  "cordova": {
    "plugins": {
      "cordova-custom-config": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "monaca-plugin-monaca-core": {}
    }
  }
}
